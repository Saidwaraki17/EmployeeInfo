        <activity android:name=".DashBoard.WorkPlandetails" />
        <activity android:name=".DashBoard.DashBoard" />
        <activity android:name=".DashBoard.Dummy" />
        <activity android:name=".DashBoard.DBScreen" />
        <activity android:name=".DashBoard.EmployeeDetails" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme1" />
        <activity
            android:name=".DashBoard.Splash"
            android:theme="@style/AppTheme1">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>